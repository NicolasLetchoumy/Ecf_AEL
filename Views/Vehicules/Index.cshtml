@model IEnumerable<ECF_auto_ecole.Models.Vehicule>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.État)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModèleVéhiculeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.État)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModèleVéhiculeNavigation.ModèleVéhicule)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NImmatriculation">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NImmatriculation">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NImmatriculation">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
